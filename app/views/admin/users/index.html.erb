<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>
<div class="container-fluid">
  <div class="row">
  
    <div class="col-3 border">
    </div>  
      
    <div class="col-9">
      
    <h2>Users List</h2>
      <table class="table table-striped">
        <thead>
          <tr>
            <th>ID</th>
            <th>Name</th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.id %></td>
              <td><%= user.name %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
      
    </div>
  </div> 
</div>