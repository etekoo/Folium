<% if @range == 'ユーザー' %>
  <h3>Users search for "<%= @word %>"</h3>
  <%= render 'public/users/index', users: @users %>
<% elsif @range == '書籍' %>
  <h3>Books search for "<%= @word %>"</h3>
  <%= render 'books/index', books: @books %>
<% elsif @range == '日記' %>
  <h3>Plant Diaries search for "<%= @word %>"</h3>
  <%= render 'public/plant_diaries/index', plant_diaries: @plant_diaries %>
<% end %>